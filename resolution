#include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main(){
	
	int n, i, c = 0;
	
	printf("Digite um numero: ");
	scanf("%d", &n);
	
	for(i = 1; i <= n; i++){
	    if (n % i == 0){;
	    c++;
	    }
	}
	if (c == 2)
	    printf("%d e um numero primo\n", n);
	else 
	    printf("%d nao e um numero primo \n", n);
	return 0;
	}
